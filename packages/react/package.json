{
  "name": "@hanghae-plus/shopping-react",
  "version": "0.0.1",
  "engines": {
    "node": ">=22",
    "pnpm": ">=10"
  },
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:ssr": "node server.js",
    "build:csr": "vite build --outDir ../../dist/react && cp ../../dist/react/index.html ../../dist/react/404.html",
    "build:ssr": "vite build --outDir ./dist/react-ssr --ssr src/main-server.tsx",
    "build:ssg": "node static-site-generate.js",
    "build": "npm run build:csr && npm run build:ssr && npm run build:ssg",
    "preview": "vite preview",
    "preview:ssr": "NODE_ENV=production node server.js",
    "preview:ssr-with-build": "pnpm run build:csr && pnpm run build:ssr && pnpm run preview:ssr",
    "tsc": "tsc --noEmit",
    "lint:fix": "eslint --fix",
    "prettier:write": "prettier --write ./src"
  },
  "dependencies": {
    "react": "latest",
    "react-dom": "latest",
    "@hanghae-plus/lib": "workspace:*"
  },
  "devDependencies": {
    "@babel/core": "latest",
    "@babel/plugin-transform-react-jsx": "latest",
    "@eslint/js": "^9.16.0",
    "@types/react": "latest",
    "@types/react-dom": "latest",
    "@types/use-sync-external-store": "latest",
    "@vitejs/plugin-react": "latest",
    "@types/node": "^24.0.13",
    "eslint": "^9.9.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "msw": "^2.10.2",
    "prettier": "^3.4.2",
    "typescript": "^5.8.3",
    "vite": "npm:rolldown-vite@latest",
    "express": "^5.1.0",
    "compression": "^1.7.5",
    "sirv": "^3.0.0"
  }
}
